<div class="artist-show-container">
  <p><%= @artist.name %>, <%= @artist.gender %></p>
  <p><%= button_to 'Follow', artist_user_artist_ratings_path(@artist), params: { rating: 1 }, class: "like-button" %></p>
  <p><%= button_to 'Skip', artist_user_artist_ratings_path(@artist), params: { rating: -1 }, class: "skip-button" %></p>
</div>
